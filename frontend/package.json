{
  "name": "cookbook-frontend",
  "version": "0.0.1",
  "scripts": {
    "dev": "parcel --hmr-port 34921 src/index.html",
    "prebuild": "rm -f dist/*",
    "build": "parcel build --no-cache src/index.html --public-url ./",
    "postbuild": "echo '⟶  bundle    =' $(du -sbh dist/ | sed 's/\\sdist\\///') && echo '⟶  bundle.gz =' $(tar -zc dist/ | wc -c | numfmt --to=iec)",
    "lint": "prettier --print-width 100 --trailing-comma all --write src/**/*.{ts,js,tsx,jsx,scss,css}",
    "bench": "find src/ -name *_bench* | xargs -n 1 ts-node -r tsconfig-paths/register -O '{\"module\":\"commonjs\"}'",
    "run": "ts-node -r tsconfig-paths/register -O '{\"module\":\"commonjs\"}'",
    "test": "find src/ -name *_test* | xargs -n 1 ts-node -r tsconfig-paths/register -O '{\"module\":\"commonjs\"}' | tap-summary",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "benchmark": "2.1.4",
    "node-sass": "4.9.0",
    "parcel-bundler": "1.7.0",
    "parcel-plugin-typescript": "0.7.1",
    "prettier": "1.12.1",
    "tap-summary": "4.0.0",
    "tape": "4.9.0",
    "ts-node": "6.0.3",
    "tsconfig-paths": "3.3.2",
    "tslint": "5.10.0",
    "typescript": "2.8.3"
  },
  "dependencies": {
    "fuse.js": "3.2.0",
    "icepick": "2.3.0",
    "reselect": "3.0.1",
    "spectre.css": "0.5.1",
    "tslib": "1.9.0",
    "ultradom": "3.2.0"
  }
}
